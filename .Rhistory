# S3 Trapezoidal Rule
trap <- function(x, y, a, b) {
n <- length(x) - 1
h <- (x[length(x)] - x[1]) / n
X <- seq(a, b, by = h)
y.indices <- which(round(x, 5) %in% round(X, 5))
Y <- y[y.indices]
n <- length(X) - 1
estInt <- h/2 * (Y[1] + sum(2*Y[2:n]) + Y[n+1])
return(estInt)
}
trap(x, y, 4, 22)
# Print method
print.trap <- function(x, y, a, b) {
estInt <- trap(x, y, a, b)
print(estInt)
}
print.trap(x, y, 4, 22)
